//program to print nth mutliplication table

#include <stdio.h>
int main() {
  int n;
  printf("Enter an integer: ");
  scanf("%d", &n);

  for (int i = 1; i <= 10; ++i) {
    printf("%d * %d = %d \n", n, i, n * i);
  }
  return 0;
}

//print even sum and odd sum from those divisible

#include<stdio.h>
main()
{
	int n,k,i,odd_sum=0,even_sum=0;
	i=1;
	printf("Enter n value");
	scanf("%d",&n);
	printf("Enter a number which multiples do you want");
	scanf("%d",&k);
	while(i<=n)
	{
		if(i%k==0)
		{
		printf("%d\n",i);
		if(i%2==0)
		even_sum=even_sum+i;
		else
		odd_sum=odd_sum+i;
		}
		i++;
	}
	printf("even multiple sum of %d is %d\n",k,even_sum);
	printf("odd multiple sum of %d is %d",k,odd_sum);
}

//print factorial of n

#include<stdio.h>  
int main()    
{    
 int i,fact=1,number;    
 printf("Enter a number: ");    
  scanf("%d",&number);    
    for(i=1;i<=number;i++){    
      fact=fact*i;    
  }    
  printf("Factorial of %d is: %d",number,fact);    
return 0;  
}

